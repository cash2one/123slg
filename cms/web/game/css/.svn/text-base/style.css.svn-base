.title .icon-green-passage { background-position: 0 -239px; }
.title .icon-author-question { background-position: 0 -120px; }
.title .icon-announcement { background-position: 0 -150px; }
.title .icon-image-appreciation { background-position: 0 -178px; }
.title .icon-game-video { background-position: 0 -267px; }
.title span { line-height: 30px; font-size: 24px; color: #333; }
.title a { font-size: 12px; line-height: 1; color: #666; position: absolute; bottom: 20px; right: 0; }
.title a:hover { color: #b80125; }
.special-system { float: right; width: 370px; }
.job-introduction { float: left; width: 541px; margin-left: 19px; display: inline; }
.job-detail { width: 539px; height: 319px; background: #fff; border: 1px solid #d7d7d7; position: relative; }
.job-tab { height: 45px; overflow: hidden; background: url("../images/job-tab_bg.png") no-repeat; }
.job-tab a { float: left; width: 70px; height: 45px; font-size: 18px; line-height: 45px; text-align: center; color: #333; position: relative; z-index: 2; }
