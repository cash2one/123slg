.quick-links span { line-height: 28px; display: block; font-size: 16px; color: #000; }
.quick-links i { width: 34px; height: 18px; display: block; border: 1px solid #b80125; border-radius: 9px; color: #b80125; line-height: 18px; text-align: center; cursor: pointer; -webkit-transition: all .6s ease; -moz-transition: all .6s ease; transition: all .6s ease; }
.quick-links a:hover i { color: #fff; background: #b80125; border: 1px solid #b80125; animation: go_anima .3s linear .2s; -webkit-animation: go_anima .3s linear .2s; }
.newsbox { padding-top: 30px; overflow: hidden; }
.game-strategy, .author-question, .announcement { width: 386px; }
.game-strategy, .author-question { float: left; }
.author-question { margin-left: 19px; display: inline; }
.announcement { float: right; }
.common-list { padding: 8px 19px; border: 1px solid #d7d7d7; }
.common-list li { height: 35px; line-height: 35px; overflow: hidden; vertical-align: middle; }
.common-list li+li { border-top: 1px dashed #d9d9d9; }
.common-list a { float: left; width: 280px; color: #333; text-overflow: ellipsis; white-space: nowrap; overflow: hidden; -webkit-transition: all .3s ease; -moz-transition: all .3s ease; transition: all .3s ease; }
.common-list a:hover { color: #be841c; }
.common-list em { float: right; font-size: 12px; color: #949494; }
.image-appreciation { width: 815px; float: left; padding-top: 30px; }
.image-list { height: 178px; overflow: hidden; }
.image-list ul { margin-left: -20px; zoom: 1; }
.image-list li { float: left; margin-left: 20px; display: inline; }
.image-list a, .image-list img { width: 258px; height: 178px; display: block; }
.image-list a { overflow: hidden; }
.image-list img { transition: all 1.2s ease; -webkit-transition: all 1.2s ease; -moz-transition: all 1.2s ease; }
.image-list a:hover img { -webkit-transform: scale(1.1, 1.1); -moz-transform: scale(1.1, 1.1); transform: scale(1.1, 1.1); }
.image-kind { height: 24px; overflow: hidden; position: absolute; top: 5px; left: 160px; }
.image-kind em { float: left; height: 18px; line-height: 18px; display: block; color: #2e2e2e; padding: 0 4px 6px; margin-right: 20px; position: relative; cursor: pointer; }
.image-kind s { width: 0; height: 3px; background: #b80125; position: absolute; bottom: 0; left: 0; -webkit-transition: all .3s ease; -moz-transition: all .3s ease; transition: all .3s ease; }
.image-kind .hover s { width: 100%; }
/*视频区域*/
.video-box { width: 370px; float: right; padding-top: 30px; }
.video-box .video-slide { height: 178px; position: relative; }
.video-box .prev-video, .video-box .next-video { display: block; width: 28px; height: 46px; background: url("../images/pre_next_btn.gif") no-repeat; position: absolute; top: 50%; margin-top: -38px; }
.video-box .vidoe_tit { width: 100%; color: #e5e5e5; text-align: center; height: 28px; line-height: 28px; position: absolute; left: 0; bottom: 0; background: rgba(0,0,0,.7); filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#b2000000, endColorstr=#b2000000);
}
.video-box .prev-video { left: 0; background-position: 0 0; }
.video-box .prev-video:hover { background-position: 0 -46px; }
.video-box .next-video { right: 0; background-position: -29px 0; }
.video-box .next-video:hover { background-position: -29px -48px; }
.video-box li { position: absolute; top: 0; left: 0; }
.video-box li a { display: block; position: relative; }
.video-box li img { display: block; width: 370px; height: 178px; }
.video-box .video-circle, .video-box .video-tran { position: absolute; left: 50%; top: 50%; background: url("../images/video_rotate.png") no-repeat; }
.video-box .video-circle { width: 68px; height: 67px; margin: -45px 0 0 -34px; background-position: 0 0; }
.video-box .video-tran { display: block; width: 24px; height: 29px; margin: -25px 0 0 -9px; background-position: -73px -17px; }
.video-box li a .video-circle { animation: rotateSelf 2s linear infinite; -moz-animation: rotateSelf 2s linear infinite; -webkit-animation: rotateSelf 2s linear infinite; -o-animation: rotateSelf 2s linear infinite; -ms-animation: rotateSelf 2s linear infinite; }
/*客服*/
.server-center { padding-top: 40px; margin-top: 60px; border-top: 1px solid #D6D6D6; overflow: hidden; zoom: 1; }
.server-center a { display: block; padding-left: 50px; height: 40px; font-size: 20px; line-height: 40px; color: #b80125; float: left; margin-right: 160px; background: url("../images/server-center.png") no-repeat; transition: all ease .4s; -webkit-transition: all ease .4s; -moz-transition: all ease .4s; }
.server-center a:hover { color: #333; }
.server-center .qq-group { background-position: 0 -42px; }
.server-center .qq-group:hover { background-position: 0 0; }
