@charset "utf-8";
body, div, dl, dt, dd, ul, li, h1, h2, h3, h4, form, input, button, textcenter-wrap, p, th, td { margin: 0; padding: 0; }
fieldset, img, input, button { font-family: "Microsoft Yahei"; border: 0; }
:link, :visited { text-decoration: none; }
ul { list-style-type: none; }
a, input { outline: none; }
html { overflow-x: hidden; }
body { font-family: "Microsoft Yahei"; background: #f7f7f7; font-size: 14px; }
em, i, s { font-weight: normal; font-style: normal; }
.center-wrap { width: 1200px; margin: 0 auto; overflow: hidden; zoom: 1; }
.center-wrap:after { content: "."; height: 0; display: block; clear: both; visibility: hidden; }
.hide { display: none; }
.text-hide { text-indent: -999px; overflow: hidden; }
.wrap { background: url("../images/bg.jpg") center top no-repeat; }
.nav { height: 72px; overflow: hidden; padding-bottom: 286px; }
.nav a { float: left; width: 166px; height: 68px; line-height: 68px; text-align: center; font-size: 18px; color: #ccc; }
.nav a.logo { width: 204px; height: 100px; }
.nav a.hover, .nav a:hover { color: #fff; background: url("../images/light.png") no-repeat; }
.nav a.logo:hover { background: none; }
.topbox { padding-top: 10px; overflow: hidden; zoom: 1; }
.topNews { float: left; width: 370px; height: 370px; background: #313438; }
.news-nav { background: #b80125; overflow: hidden; zoom: 1; }
.news-nav li { float: left; padding: 0 15px; font-size: 18px; color: #fff; line-height: 40px; cursor: pointer; }
.news-nav li.hover { background: #8e0d29; color: #fff; }
.news-nav .more-news { float: right; margin-right: 10px; line-height: 40px; color: #8c8c8c; }
.news-nav .more-news:hover { color: #fff; }
.news-tab { padding: 0 20px; }
.tab-list h3 { width: 300px; padding: 0 15px; height: 30px; font-weight: normal; margin: 20px auto 10px; background: #24272a; overflow: hidden; zoom: 1; }
.tab-list h3 a { display: block; width: 300px; line-height: 30px; text-align: center; color: #b2b2b2; overflow: hidden; white-space: nowrap; text-overflow: ellipsis; }
.tab-list h3 a:hover { color: #b80125; }
.tab-list li { height: 32px; line-height: 32px; overflow: hidden; padding-left: 6px; vertical-align: middle; position: relative; -webkit-transition: all 0.3s ease; -moz-transition: all 0.3s ease; transition: all 0.3s ease; }
.tab-list .square { width: 2px; height: 2px; display: block; background: #b2b2b2; position: absolute; top: 17px; left: 1px; }
.tab-list li:hover { padding-left: 12px; }
.tab-list li:hover a { color: #b80125; }
.tab-list a { float: left; width: 270px; color: #b2b2b2; text-overflow: ellipsis; white-space: nowrap; overflow: hidden; }
.tab-list em { float: right; color: #737373; }
/*轮播*/
.focusImg { float: left; width: 560px; height: 370px; overflow: hidden; position: relative; }
.focusImg ul { height: 290px; position: absolute; left: 0; top: 0; }
.focusImg li { float: left; }
.focusImg li, .focusImg img { width: 560px; height: 290px; }
.focusImg i { width: 30px; height: 66px; display: none; cursor: pointer; background: url("../images/arrow.png") no-repeat; position: absolute; top: 127px; z-index: 3; }
.focusImg i.btn-prev { background-position: left top; left: 0; }
.focusImg i.btn-next { background-position: right top; right: 0; }
.handle { height: 12px; overflow: hidden; position: absolute; bottom: 95px; left: 15px; z-index: 3; }
.handle span { float: left; width: 12px; height: 12px; display: block; cursor: pointer; background: #d1d1d1; margin-right: 5px; border-radius: 5px; text-indent: -99px; overflow: hidden; -webkit-transition: all 0.3s ease; -moz-transition: all 0.3s ease; transition: all 0.3s ease; }
.handle span.cur { width: 28px; background: #b80125; }
.activ-link { width: 570px; position: absolute; bottom: 0; left: 0; overflow: hidden; zoom: 1; }
.activ-link a { display: block; width: 186px; height: 79px; margin-right: 1px; float: left; transition: all .4s ease; -webkit-transition: all .4s ease; -moz-transition: all .4s ease; }
.activ-link a:hover { opacity: .85; filter: alpha(opacity=85); }
.activ-link a img { width: 186px; height: 79px; }
/*左侧服务器列表，开始下载按钮*/
.topbox .server-box { float: left; width: 270px; height: 370px; }
.btns { width: 270px; height: 186px; position: relative; }
.btns a { display: block; background: url("../images/down_char_bg.gif") no-repeat; overflow: hidden; position: absolute; }
.btns .btn-start { width: 270px; height: 144px; background: url("../images/start_btn.jpg") no-repeat center center; top: 0; left: 0; }
.btns .start-txt, .start-txt::after { content: ""; display: block; width: 186px; height: 68px; color: #360202; font-size: 16px; text-align: center; position: absolute; top: 45px; left: 50%; margin-left: -93px; background: url("../images/start_btn_txt.png") no-repeat center top; text-shadow: 1px 1px 1px #fff; -moz-text-shadow: 1px 1px 1px #fff; -webkit-text-shadow: 1px 1px 1px #fff; }
.start-txt::after { top: 0; }
.btns .btn-start .start-txt { animation: btn_anima_01 1.6s linear infinite; -webkit-animation: btn_anima_01 1.6s linear infinite; }
.btns .btn-start .start-txt::after { animation: btn_anima_02 1.6s linear infinite; -webkit-animation: btn_anima_02 1.6s linear infinite; }
.btns .btn-download { width: 135px; height: 42px; background-position: 0 0; bottom: 0; left: 0; text-indent: -9999em; }
.btns .btn-recharge { width: 135px; height: 42px; background-position: -135px 0; bottom: 0; right: 0; text-indent: -9999em; }
.btns .btn-start:hover { background: url("../images/start_btn_hover.jpg") no-repeat center center; }
.btns .btn-download:hover { background-position: 0 -42px; }
.btns .btn-recharge:hover { background-position: -135px -42px; }
.server-list { padding: 20px 20px 9px; background: #4d4f53; }
.server-list dl { height: 155px; overflow: hidden; zoom: 1; }
.server-list dt, .server-list dd { overflow: hidden; zoom: 1; vertical-align: top; }
.server-list dt { padding-right: 10px; margin-bottom: 5px; background: #333436; }
.server-list a { float: right; color: #fff; font-size: 12px; line-height: 30px; }
.server-list a:hover { color: #fed976; }
.server-list dt h3, .server-list span, .server-list em { float: left; font-size: 12px; line-height: 30px; }
.server-list dt h3 { padding: 0 10px; font-size: 16px; font-weight: normal; color: #fff; line-height: 30px; background: #1a1a1a; }
.server-list span { display: block; width: 130px; color: #fff; }
.server-list em { margin-right: 15px; color: #fff; position: relative; }
