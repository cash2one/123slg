.server-list .hot_game s { display: block; background-position: 0 0; }
.server-list .new_game s { display: block; background-position: 0 -11px; }
.complex { padding-top: 30px; position: relative; }
.title { height: 30px; padding: 0 0 20px 36px; position: relative; }
