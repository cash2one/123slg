.quick-links { overflow: hidden; }
.quick-links li { float: left; }
.quick-links a { width: 170px; height: 92px; display: block; padding: 15px 0 0 15px; position: relative; }
.quick-links a img { position: absolute; top: 0; left: 0; z-index: 0; }
